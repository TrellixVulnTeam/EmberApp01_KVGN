<div class="panel">
        <div class="panel-heading">
            <h4 class="panel-tittle">
                <a data-toggle="collapse" href="#a{{item.id}}">
                 {{item.nombre}}
                </a>
            </h4>
        </div>
        <div id="a{{item.id}}" class="panel-collapse collapse">
            <div class="panel-body">
                Inicio: {{item.inicio}}<br>
                Fin: {{item.fin}}
            </div>
        </div>
    </div>